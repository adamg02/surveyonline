    "seed": "tsx ./src/scripts/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "latest",
    "express": "^4.19.2",
    "zod": "^3.23.8",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "prisma": "latest",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.7",
    "supertest": "^7.0.0",
    "@types/supertest": "^2.0.16",
    "@types/jsonwebtoken": "^9.0.6",
  "ts-node": "^10.9.2",
  "tsx": "^4.19.2"
  }
