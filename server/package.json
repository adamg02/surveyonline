{
  "name": "surveyonline-server",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prebuild": "node -e \"const fs = require('fs'); const path = 'dist'; if (fs.existsSync(path)) fs.rmSync(path, {recursive: true}); console.log('dist folder clean');\"",
    "build": "tsc -p tsconfig.json",
    "postbuild": "prisma generate",
    "start": "node dist/index.js",
    "deploy": "prisma migrate deploy && npm run seed:prod",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "test": "tsx ./test/basic.test.ts",
    "seed": "tsx ./src/scripts/seed.ts",
    "seed:prod": "node dist/scripts/seed.js"
  },
  "dependencies": {
    "@prisma/client": "latest",
    "express": "^4.19.2",
    "zod": "^3.23.8",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "typescript": "^5.4.0"
  },
  "devDependencies": {
    "prisma": "latest",
    "ts-node-dev": "^2.0.0",
    "@types/uuid": "^9.0.7",
    "supertest": "^7.0.0",
    "@types/supertest": "^2.0.16",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2"
  }
}